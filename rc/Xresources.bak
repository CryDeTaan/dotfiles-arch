URxvt.urgentOnBell: true
URxvt.scrollBar: false
URxvt.font: xft:roboto:size=10
URxvt.depth: 24
URxvt.url-launcher: /usr/bin/chrome
URxvt.matcher.button: 1
URxvt.perl-ext-common: default,matcher

urxvt*termName: rxvt

Xft*dpi:96
Xft*antialias: true
Xft*hinting: full

URxvt.letterSpace: -2

URxvt.termName: rxvt-unicode

urxvt*transparent: true
urxvt*shading: 30
URxvt.tintColor: white
